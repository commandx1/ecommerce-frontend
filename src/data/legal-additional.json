  "regulatoryNotices": {
    "id": "regulatory-notices",
    "title": "Regulatory Notices",
    "subtitle": "FDA and industry compliance information",
    "icon": "gavel",
    "notice": {
      "type": "error",
      "title": "Important Regulatory Notice",
      "text": "All products sold through DentalHub must comply with FDA regulations and relevant industry standards. Buyers are responsible for ensuring proper use and compliance."
    },
    "sections": [
      {
        "title": "FDA Medical Device Regulations",
        "deviceClasses": [
          {
            "class": "Class I Devices",
            "items": [
              "General controls apply",
              "Usually exempt from premarket review",
              "Basic safety and effectiveness standards",
              "Examples: dental floss, mouth mirrors"
            ]
          },
          {
            "class": "Class II Devices",
            "items": [
              "General and special controls",
              "Usually require 510(k) clearance",
              "Performance standards required",
              "Examples: dental X-ray machines, amalgam"
            ]
          },
          {
            "class": "Class III Devices",
            "items": [
              "Highest level of regulatory control",
              "Usually require PMA approval",
              "Clinical trials often necessary",
              "Examples: dental implants, certain lasers"
            ]
          }
        ]
      },
      {
        "title": "Quality System Regulations",
        "content": "All medical device manufacturers must comply with FDA Quality System Regulation (QSR) requirements, including:",
        "items": [
          "Design controls and validation",
          "Document and record control systems",
          "Management responsibility and organization",
          "Resource management and training",
          "Product realization and manufacturing",
          "Measurement, analysis, and improvement",
          "Corrective and preventive action (CAPA)",
          "Risk management and post-market surveillance"
        ]
      },
      {
        "title": "State Licensing Requirements",
        "content": "Dental professionals must maintain current state licenses and comply with state-specific regulations regarding the purchase and use of dental products and equipment. Some products may require special permits or certifications."
      }
    ]
  },
